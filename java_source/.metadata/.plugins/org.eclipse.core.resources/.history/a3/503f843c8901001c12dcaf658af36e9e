package chapter2;

public class CastEx3 {
	public static void main(String[] args) {
		int num1 = 10;
		double num2 = 2.0;
	    
		System.out.println((int)(num1 + num2));
		System.out.println((int)(num1 - num2));
		System.out.println((int)(num1 / num2));
		System.out.println((int)(num1 * num2));
		
		char ch1 = 'วั';
		System.out.println(ch1);
	}
}
