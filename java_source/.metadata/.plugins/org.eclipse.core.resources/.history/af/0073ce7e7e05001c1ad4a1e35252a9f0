package test20210825;

public class Q1 {
	public static void main(String[] args) {
		int num1 = 20;
		double num2 = 3.0;
		
		System.out.println("엇핌 : " + (int)(num1 + num2));
		System.out.println("질쐑 : " + (int)(num1 - num2));
		System.out.println("질쐑 : " + (int)(num1 - num2));
		System.out.println("질쐑 : " + (int)(num1 - num2));
	}
}
